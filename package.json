{
  "name": "sc-planner",
  "version": "0.4.0",
  "description": "SC Planner",
  "scripts": {
    "setup": "chmod +x scripts/setup-git.sh && ./scripts/setup-git.sh",
    "ssh": "eval $(ssh-agent) ssh-add ~/.ssh/perso",
    "check": "node scripts/check-setup.js",
    "dev": "docker compose -f docker-compose.dev.yml up -d --build",
    "prod": "docker compose -f docker-compose.prod.yml up -d --build",
    "down": "docker compose -f docker-compose.dev.yml down",
    "logs": "docker compose logs -f",
    "release": "node scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomasFourties/sc-planner.git"
  },
  "keywords": [
    "nuxt",
    "nestjs",
    "postgresql",
    "docker",
    "starter",
    "changelog",
    "conventional-commits"
  ],
  "author": "Thomas Fourties",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThomasFourties/sc-planner/issues"
  },
  "homepage": "https://github.com/ThomasFourties/sc-planner#readme",
  "devDependencies": {
    "@octokit/rest": "^22.0.0",
    "conventional-changelog-cli": "^3.0.0",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "packageFiles": [
      "package.json",
      "client/package.json",
      "server/package.json"
    ],
    "bumpFiles": [
      "package.json",
      "client/package.json",
      "server/package.json"
    ]
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
